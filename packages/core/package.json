{
  "name": "@cfgkit/core",
  "version": "0.0.0",
  "description": "Core package for cfgkit",
  "homepage": "https://docs.muijf.com/cfgkit",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muijf/cfgkit.git",
    "directory": "packages/core"
  },
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup"
  },
  "dependencies": {
    "@cfgkit/shared": "workspace:*",
    "@cfgkit/typescript": "workspace:*",
    "@standard-schema/spec": "^1.0.0",
    "glob": "^11.0.1"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "tsup": "^8.4.0"
  },
  "files": [
    "dist"
  ]
}
